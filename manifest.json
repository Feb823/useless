{
  "manifest_version": 3,
  "name": "Thought Bubble Annihilator",
  "version": "1.0",
  "description": "An app that floods your screen with distractions when you pause to think.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {}
}